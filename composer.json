{
  "name": "bigbank/digidoc",
  "description": "Library to interface with the Estonian national identification platform",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Bigbank\\DigiDoc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bigbank\\DigiDoc\\Test\\": "tests/"
    }
  },
  "require": {
    "ext-xml": "*",
    "ext-curl": "*",
    "ext-soap": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "php": ">=5.6",
    "league/container": "~2.0"
  },
  "require-dev": {
    "wsdl2phpgenerator/wsdl2phpgenerator": "~3",
    "phpunit/phpunit":"~5"
  },
  "scripts": {
    "post-update-cmd": "bin/wsdl_generator",
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
